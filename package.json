{
	"name": "@avanio/ip-block",
	"version": "0.0.1",
	"description": "Util to block IP",
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "npm run build",
		"test": "nyc mocha",
		"coverage": "nyc report --reporter=lcovonly",
		"lint": "eslint src"
	},
	"files": [
		"dist"
	],
	"mocha": {
		"exit": true,
		"extension": [
			"ts",
			"js"
		],
		"recursive": true,
		"require": [
			"ts-node/register",
			"source-map-support/register"
		],
		"reporters": [
			"spec",
			"mocha-junit-reporter"
		]
	},
	"nyc": {
		"check-coverage": true,
		"extension": [
			".ts",
			".js"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AvanioOy/ip-block.git"
	},
	"keywords": [
		"ip",
		"block"
	],
	"author": "mharj",
	"license": "LGPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/AvanioOy/ip-block/issues"
	},
	"homepage": "https://github.com/AvanioOy/ip-block#readme",
	"devDependencies": {
		"@types/chai": "^4.3.5",
		"@types/chai-as-promised": "^7.1.5",
		"@types/mocha": "^10.0.1",
		"@types/node": "^20.4.5",
		"@types/sinon": "^10.0.16",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"chai": "^4.3.7",
		"chai-as-promised": "^7.1.1",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-deprecation": "^1.5.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-sonarjs": "^0.19.0",
		"mocha": "^10.2.0",
		"nyc": "^15.1.0",
		"prettier": "^3.0.0",
		"sinon": "^15.2.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"@avanio/expire-cache": "^0.3.2",
		"@avanio/logger-like": "^0.1.1",
		"mharj-result": "^0.5.1"
	}
}
